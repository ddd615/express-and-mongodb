<!DOCTYPE html>
<html>
<head>
    <title></title>

</head>
<body>
<table>
    <thead>
    <tr>
        <th>标题</th>
        <th>作者</th>
        <th>来源</th>
        <th>发布时间</th>
        <th>操作</th>
    </tr>
    </thead>
    <tbody>
    <%for(var i in list) {%>
    <tr>
        <td><%=list[i].title%></td>
        <td><%=list[i].author%></td>
        <td><%=list[i].from%></td>
        <td><%=list[i].time%></td>
        <td>
            <a href="/edit_news.html?id=<%=list[i].id%>">修改</a>
            <a href="/del_news.html?id=<%=list[i].id%>">删除</a>
        </td>
    </tr>
    <%}%>
    </tbody>
</table>
</body>
</html>
